def setup ():
    size(400,400)            #Définition de la Taille du fond d'ecran
    background(10,190,248)   #Couleur du fond d'écran (R;V;B) Jusqu'a 255

def draw ():
    fill(245,30,40)        #Choix de la couleur de la figure géométrique.
    rect(300,300,100,100) # Position(x;y) puis Taille (Horizontale;Verticale)
    
    